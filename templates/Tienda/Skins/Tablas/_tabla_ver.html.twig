<table class="text-center table table-bordered">
    <thead>
    <tr>
        <td>Nombre Skin</td>
        <td>Precio</td>
    </tr>
    </thead>
    <tbody>
        {% for skin in skins %}
            <a href="{{ path('ver', {'id':skin.id} ) }}"><tr>
                <td class="active">{{ skin.nombreSkin }}</td>
                <td class="info">{{ skin.precio }}</td>
            </tr></a>
        {% endfor %}
    </tbody>
</table>